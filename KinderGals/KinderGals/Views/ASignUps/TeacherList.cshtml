@model IEnumerable<KinderGals.Models.TSignUp>

@{
      

                ViewBag.Title = "TeacherList";
}

<head>

    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="~/images/apple-icon.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/images/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>Dashboard</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


    <!-- Bootstrap core CSS     -->
    <link href="~/css/2bootstrap.min.css" rel="stylesheet" />

    <!-- Animation library for notifications   -->
    <link href="~/css/2animate.min.css" rel="stylesheet" />

    <!--  Paper Dashboard core CSS    -->
    <link href="~/css/2paper-dashboard.css" rel="stylesheet" />


    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="~/css/2demo.css" rel="stylesheet" />


    <!--  Fonts and icons     -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
    <link href="~/css/2themify-icons.css" rel="stylesheet">

</head>

<body style="background: url(https://d1yn1kh78jj1rr.cloudfront.net/image/preview/HqviomN9gizquzdm9/graphicstock-border-template-with-kids-painting-illustration_rxQoJVq8jx_SB_PM.jpg); background-size: 100% 100%;">

    <div class="wrapper">
        <div class="sidebar" data-background-color="white" data-active-color="danger">

            <!--
                Tip 1: you can change the color of the sidebar's background using: data-background-color="white | black"
                Tip 2: you can change the color of the active button using the data-active-color="primary | info | success | warning | danger"
            -->

            <div class="sidebar-wrapper">
                <div class="logo">
                    <a class="simple-text">
                        KINDERGALS
                    </a>
                </div>

                <ul class="nav">
                    <li class="active">
                        <a href="@Url.Action("AdminDashBoard", "ASignUps")">
                            <i class="ti-panel"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("StudentList", "ASignUps")">
                            <i class="ti-user"></i>
                            <p>Student List</p>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("TeacherList", "ASignUps")">
                            <i class="ti-view-list-alt"></i>
                            <p>Teacher List</p>
                        </a>
                    </li>






                </ul>
            </div>
        </div>

        <div class="main-panel">

            <nav class="navbar navbar-default">

                <div class="container-fluid">




                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar bar1"></span>
                            <span class="icon-bar bar2"></span>
                            <span class="icon-bar bar3"></span>
                        </button>
                        <a class="navbar-brand" href="Assignments.html"></a>
                    </div>




                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar bar1"></span>
                            <span class="icon-bar bar2"></span>
                            <span class="icon-bar bar3"></span>
                        </button>
                        <a class="navbar-brand" href="Announcement.html"></a>
                    </div>


                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar bar1"></span>
                            <span class="icon-bar bar2"></span>
                            <span class="icon-bar bar3"></span>
                        </button>

                    </div>

                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">


                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle"></button>
                                <a class="navbar-brand" href="@Url.Action("Index", "Home")">LOG OFF</a>
                            </div>



                        </ul>

                    </div>
                </div>
            </nav>


            <table class="table">
                <tr>
                    <th style= "color:black">
                        @Html.DisplayNameFor(model => model.UserID)
                    </th>
                    <th style= "color:black">
                        @Html.DisplayNameFor(model => model.Name)
                    </th>
                    <th style= "color:black">
                        @Html.DisplayNameFor(model => model.Password)
                    </th>
                   
                    <th style= "color:black">
                        @Html.DisplayNameFor(model => model.SectionA)
                    </th>
                    <th style= "color:black">
                        @Html.DisplayNameFor(model => model.SectionB)
                    </th>
                    <th style= "color:black">
                        @Html.DisplayNameFor(model => model.SectionC)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr style= "color:black">
                        <td style= "color:black">
                            @Html.DisplayFor(modelItem => item.UserID)
                        </td>
                        <td style= "color:black">
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td style= "color:black">
                            @Html.DisplayFor(modelItem => item.Password)
                        </td>
                       
                        <td style= "color:black">
                            @Html.DisplayFor(modelItem => item.SectionA)
                        </td>
                        <td style= "color:black">
                            @Html.DisplayFor(modelItem => item.SectionB)
                        </td>
                        <td style= "color:black">
                            @Html.DisplayFor(modelItem => item.SectionC)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                            @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.ID })
                        </td>
                    </tr>
                }

            </table>
        </div>
</body>
<script src="~/js/2jquery.min.js" type="text/javascript"></script>
<script src="~/js/2bootstrap.min.js" type="text/javascript"></script>
<!--  Checkbox, Radio & Switch Plugins -->
<script src="~/js/2bootstrap-checkbox-radio.js"></script>
<!--  Charts Plugin -->
<script src="~/js/2chartist.min.js"></script>
<!--  Notifications Plugin    -->
<script src="~/js/2bootstrap-notify.js"></script>
<!--  Google Maps Plugin    -->
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>
<!-- Paper Dashboard Core javascript and methods for Demo purpose -->
<script src="~/js/2paper-dashboard.js"></script>
<!-- Paper Dashboard DEMO methods, don't include it in your project! -->
<script src="~/js/2demo.js"></script>

